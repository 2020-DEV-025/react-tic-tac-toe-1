{
  "name": "tic-tac-toe",
  "version": "1.0.0",
  "description": "A React implementation of Tic Tac Toe",
  "main": "dist/bundle.js",
  "scripts": {
    "start": "webpack-dev-server --progress --colors --hot --content-base public/",
    "build": "webpack --progress --colors",
    "test": "find ./ -name '*.spec.js' | xargs mocha -R min -r babel/register",
    "tdd": "webpack-dev-server --config test/webpack.test.config.js",
    "deploy": "NODE_ENV=production webpack -p --config webpack.production.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/negomi/tic-tac-toe.git"
  },
  "keywords": [
    "tic tac toe",
    "react-component"
  ],
  "author": "Imogen Wentworth",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/negomi/tic-tac-toe/issues"
  },
  "homepage": "http://negomi.github.io/tic-tac-toe/",
  "dependencies": {
    "normalize.css": "^3.0.3",
    "react": "^0.14.0-rc1",
    "react-dom": "^0.14.0-rc1"
  },
  "devDependencies": {
    "autoprefixer": "^6.0.3",
    "babel": "^5.8.23",
    "babel-core": "^5.8.24",
    "babel-eslint": "^4.1.3",
    "babel-loader": "^5.3.2",
    "chai": "^3.2.0",
    "css-loader": "^0.19.0",
    "eslint": "^1.6.0",
    "eslint-plugin-react": "^3.4.2",
    "file-loader": "^0.8.4",
    "mocha": "^2.3.2",
    "mocha-loader": "^0.7.1",
    "node-sass": "^3.3.3",
    "postcss-loader": "^0.6.0",
    "react-addons-test-utils": "^0.14.0-rc1",
    "sass-loader": "^3.0.0",
    "style-loader": "^0.12.4",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.2",
    "webpack-dev-server": "^1.11.0"
  }
}
